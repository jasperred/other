<%@ taglib prefix="s" uri="/struts-tags"%>
<table>
	<tr>
		<td>
			<label>用户:</label>
		</td><td>
		<input type="hidden" disabled="disabled" id="mp-user-id" value='<s:property value="userId"/>'/>
			<span><input type="text" disabled="disabled" id="mp-user-name" value='<s:property value="userNo"/>'/> </span>
		</td>
	</tr>
	<tr>
		<td>
			<label>原密码:</label>
		</td><td>
			<span><input type="password" id="mp-old-pwd" /> </span>
		</td>
	</tr>
	<tr>
		<td>
			<label>新密码:</label>
		</td><td>
			<span><input type="password" id="mp-new-pwd" onchange="checkValueLength($(this),60)" /> </span>
		</td>
	</tr>
	<tr>
		<td>
			<label>确认密码:</label>
		</td><td>
			<span><input type="password" id="mp-renew-pwd" /> </span>
		</td>
	</tr>
	<tr>
		<td>
		</td>
	</tr>
</table>
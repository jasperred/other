<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.web.entity">
	<class
		name="Invoice"
		table="invoice"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>
 
		<id
			name="Id"
			type="integer"
			column="invoice_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="InvoiceCd"
			column="invoice_cd"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="CompanyId"
			column="company_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="FromDate"
			column="from_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="EndDate"
			column="end_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="ServiceAmt"
			column="service_amt"
			type="big_decimal"
			not-null="false"
			length="18"
		/>
		<property
			name="HomeAmt"
			column="home_amt"
			type="big_decimal"
			not-null="false"
			length="18"
		/>
		<property
			name="OtherAmt"
			column="other_amt"
			type="big_decimal"
			not-null="false"
			length="18"
		/>
		<property
			name="TotalAmt"
			column="total_amt"
			type="big_decimal"
			not-null="false"
			length="18"
		/>
		<property
			name="InvoiceDate"
			column="invoice_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="InvoiceStatus"
			column="invoice_status"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="InvoiceNoticeDate"
			column="invoice_notice_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="InvoiceNoticePerson"
			column="invoice_notice_person"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="InvoiceReceiveDate"
			column="invoice_receive_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="PaymentDate"
			column="payment_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="InvoiceUrl"
			column="invoice_url"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Memo"
			column="memo"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Ctime"
			column="ctime"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Mtime"
			column="mtime"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Cuser"
			column="cuser"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Muser"
			column="muser"
			type="string"
			not-null="false"
			length="30"
		/>


	</class>	
</hibernate-mapping>
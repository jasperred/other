<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.web.entity">
	<class
		name="ModuleConfig"
		table="module_config"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="module_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="ModuleName"
			column="module_name"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="ModuleAlias"
			column="module_alias"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="ModuleType"
			column="module_type"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="ParentModuleId"
			column="parent_module_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="ShowMenu"
			column="show_menu"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="UrlParam"
			column="url_param"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Url"
			column="url"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Sequence"
			column="sequence"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="PermissionId"
			column="permission_id"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="ImagePath"
			column="image_path"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Ctime"
			column="ctime"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="Mtime"
			column="mtime"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="Cuser"
			column="cuser"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Muser"
			column="muser"
			type="string"
			not-null="false"
			length="20"
		/>


	</class>	
</hibernate-mapping>
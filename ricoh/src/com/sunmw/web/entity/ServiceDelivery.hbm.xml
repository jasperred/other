<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.web.entity">
	<class
		name="ServiceDelivery"
		table="service_delivery"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="service_delivery_id"
		>
			<generator class="identity"/>
		</id>

		
		<property
			name="ServiceDeliveryNo"
			column="service_delivery_no"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="ServiceApplyId"
			column="service_apply_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="ServiceId"
			column="service_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="ServiceNo"
			column="service_no"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="CompanyId"
			column="company_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Department"
			column="department"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="ShippingNo"
			column="shipping_no"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="DeliveryStock"
			column="delivery_stock"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="DeliveryDate"
			column="delivery_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="AuditedTime"
			column="audited_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="DeliveryTime"
			column="delivery_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="AllTime"
			column="all_time"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Ctime"
			column="ctime"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Cuser"
			column="cuser"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Mtime"
			column="mtime"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Muser"
			column="muser"
			type="string"
			not-null="false"
			length="50"
		/>


	</class>	
</hibernate-mapping>
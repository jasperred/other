<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.web.entity">
	<class
		name="ServiceDeliveryItem"
		table="service_delivery_item"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="service_delivery_item_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="ServiceDeliveryId"
			column="service_delivery_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="ItemType"
			column="item_type"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="ItemCate"
			column="item_cate"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="ItemName"
			column="item_name"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="ItemNo"
			column="item_no"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="ItemQty"
			column="item_qty"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="ItemSerialNo"
			column="item_serial_no"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="ItemChange"
			column="item_change"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="NewItemNo"
			column="new_item_no"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="HasInv"
			column="has_inv"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="ItemOrderNo"
			column="item_order_no"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="ItemOrderDate"
			column="item_order_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="ItemDeliveryDate"
			column="item_delivery_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="ItemArrivalDate"
			column="item_arrival_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Ctime"
			column="ctime"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Cuser"
			column="cuser"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Mtime"
			column="mtime"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Muser"
			column="muser"
			type="string"
			not-null="false"
			length="50"
		/>

	</class>	
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.web.entity">
	<class
		name="Machine"
		table="machine"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="machine_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="OutDate"
			column="out_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="MachineCd"
			column="machine_cd"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="MachineTypeId"
			column="machine_type_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="MachineNo"
			column="machine_no"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="PpsNo"
			column="pps_no"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="ServiceFromDate"
			column="service_from_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="ServiceEndDate"
			column="service_end_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="InvoiceDate"
			column="invoice_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="PaymentStatus"
			column="payment_status"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="PaymentCompany"
			column="payment_company"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="PaymentCompanyCd"
			column="payment_company_cd"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="PaymentDate"
			column="payment_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="PaymentAmt"
			column="payment_amt"
			type="big_decimal"
			not-null="false"
			length="18"
		/>
		<property
			name="CustomerNo"
			column="customer_no"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="CustomerName"
			column="customer_name"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Ctime"
			column="ctime"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Mtime"
			column="mtime"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Cuser"
			column="cuser"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Muser"
			column="muser"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="LastTime"
			column="last_time"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="LastCompanyId"
			column="last_company_id"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Origin"
			column="origin"
			type="string"
			not-null="false"
			length="30"
		/>


	</class>	
</hibernate-mapping>
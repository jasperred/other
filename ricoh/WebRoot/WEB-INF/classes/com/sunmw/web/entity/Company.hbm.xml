<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sunmw.web.entity">
	<class
		name="Company"
		table="company"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="Id"
			type="integer"
			column="company_id"
		>
			<generator class="identity"/>
		</id>
		<property
			name="CompanyNo"
			column="company_no"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="CompanyName"
			column="company_name"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Category"
			column="category"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="RcnNo"
			column="rcn_no"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="ProvinceCd"
			column="province_cd"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="City"
			column="city"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Address"
			column="address"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Zip"
			column="zip"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Phone"
			column="phone"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Contact"
			column="contact"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Ctime"
			column="ctime"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Mtime"
			column="mtime"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Cuser"
			column="cuser"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Muser"
			column="muser"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Status"
			column="status"
			type="string"
			not-null="false"
			length="30"
		/>


	</class>	
</hibernate-mapping>
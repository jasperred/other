<?xml version="1.0" encoding="UTF-8"?>
<taglib version="2.0" xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd">
	<description>"To upload a file using ajax and to monitor its
		progress"</description>
	<display-name>"Ajax File Upload Tags"</display-name>
	<tlib-version>1.0</tlib-version>
	<short-name>djc</short-name>
	<uri>http://www.devedup.com/taglibs</uri>
	<tag>
		<name>ajaxfileuploadform</name>
		<tag-class>com.devedup.ajaxfileupload.views.jsp.ui.ProgressBarTag
		</tag-class>
		<body-content>empty</body-content>
		<description>Renders an file input form with cooperating
			progress div</description>
		<attribute>
			<description>Set action nane to submit to, without .action suffix
			</description>
			<name>action</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>The javascript function to do before the file upload
			</description>
			<name>dobefore</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>The javascript function to do after success of file
				upload</description>
			<name>doafter</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>The css class to use for element</description>
			<name>cssClass</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>The css style definitions for element ro use
			</description>
			<name>cssStyle</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>head</name>
		<tag-class>com.devedup.ajaxfileupload.views.jsp.ui.HeadTag
		</tag-class>
		<body-content>empty</body-content>
		<description>Renders the javascript and css includes needed for
			the file upload</description>
	</tag>
</taglib>
